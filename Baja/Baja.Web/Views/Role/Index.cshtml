@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityRole>

@{
    ViewBag.Title = "Roles";
}

<h2>Index</h2>

<h2>Available Roles For Application</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table id="tbrole" class="table">
    <tr>
        <th>
            Role Name
        </th>
        <th>

        </th>
    </tr>
    @foreach (var item in Model)
    {
        
        <tr>
            <td>
                @item.Name
            </td>
            
            <td>
                @*@Html.ActionLink("Edit", "Edit", new { name = item.Name }) |*@
                @Html.ActionLink("Delete", "Delete", new { name = item.Name })
            </td>
            
        </tr>
    }
</table>